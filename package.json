{
  "name": "pet-food-tracker",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3001",
  "author": {
    "name": "Craig M Andersen",
    "email": "andersen.craigm@gmail.com",
    "url": "https://craigandersen.io"
  },
  "scripts": {
    "dev": "concurrently \"yarn:dev-*\"",
    "dev-client": "react-scripts start",
    "dev-server-dist": "NODE_ENV=development nodemon server/build/index.js",
    "dev-server-src": "cd server/ && tsc -w",
    "build-client": "react-scripts build",
    "build-server": "cd server/ && tsc",
    "build": "concurrently \"yarn build-client\" \"yarn build-server\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "NODE_ENV=production pm2 start server/build/index.js --name tracker",
    "restart": "pm2 restart tracker",
    "logs": "pm2 logs tracker --lines 100",
    "stop": "pm2 delete tracker"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "@types/express": "^4.17.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/pg": "^7.11.1",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "ask-sdk": "^2.7.0",
    "ask-sdk-core": "^2.7.0",
    "ask-sdk-express-adapter": "^2.0.0",
    "ask-sdk-model": "^1.19.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "express": "^4.17.1",
    "konphyg": "^1.5.0",
    "node-sass": "^4.12.0",
    "nodemailer": "^6.4.16",
    "number-to-words": "^1.2.4",
    "pg": "^7.12.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.2",
    "twilio": "^3.35.0",
    "typescript": "3.6.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.2"
  }
}
